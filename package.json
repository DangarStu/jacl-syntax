{
  "name": "jacl",
  "displayName": "JACL",
  "description": "Syntax highlighting if JACL.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DangarStu/jacl-syntax"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jacl",
      "aliases": ["JACL", "jacl"],
      "extensions": [".jacl",".j2"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jacl",
      "scopeName": "source.jacl",
      "path": "./syntaxes/jacl.tmLanguage.json"
    }]
  }
}
